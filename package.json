{
  "name": "cadept",
  "version": "1.2.0",
  "description": "Callable Async DEPendency Task",
  "files": [
    "src",
    "bundle.js",
    "bundle.js.map"
  ],
  "exports": {
    "import": "./src/Task.js",
    "require": "./bundle.js"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "setup": "node setup.js",
    "watch": "npm-watch",
    "test": "nyc mocha --require source-map-support/register",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "rollup -c",
    "clean": "rm -f bundle.js bundle.js.map && find . -name '.DS_Store' -not -path '*/node_modules/*' | xargs rm -r",
    "docs": "npm run docs:gen && npm run docs:publish",
    "docs:gen": "npm run docs:clean && jsdoc -c jsdoc.json && cd docs/cadept && ln -sfn $npm_package_version latest",
    "docs:clean": "rm -rf docs/cadept/$npm_package_version",
    "docs:publish": "git subtree push --prefix docs/cadept origin gh-pages"
  },
  "_setup": {
    "deps": {
      "nyc": {
        "version": "^15.1.0"
      },
      "npm-watch": {
        "version": "^0.7.0"
      },
      "codecov": {
        "version": "^3.8.1"
      },
      "rollup": {
        "version": "^2.34.0"
      },
      "mocha": {
        "version": "^8.2.1"
      },
      "source-map-support": {
        "version": "^0.5.19"
      }
    }
  },
  "watch": {
    "test": {
      "patterns": [
        "test.js",
        "bundle.js"
      ],
      "inherit": true
    },
    "build": {
      "patterns": "src",
      "inherit": true
    },
    "docs:gen": {
      "patterns": [
        "src",
        "README.md",
        "jsdoc.json"
      ],
      "inherit": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amekusa/cadept.git"
  },
  "keywords": [
    "async",
    "dependency",
    "task",
    "callable"
  ],
  "author": "amekusa (https://amekusa.com)",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "conso1e": "^1.2.2",
    "flex-params": "^3.0.1",
    "generic-exceptions": "^3.0.2"
  },
  "devDependencies": {
    "docolatte": "^2.10.2",
    "source-map-support": "^0.5.19",
    "sourcemap-validator": "^2.1.0"
  }
}
