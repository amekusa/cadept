{
  "name": "cadept",
  "version": "1.0.0",
  "description": "Callable Async DEPendency Task",
  "files": [
    "src",
    "bundle.js"
  ],
  "exports": {
    "import": "./src/Task.js",
    "require": "./bundle.js"
  },
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "setup": "node setup.js",
    "watch": "npm-watch",
    "test": "nyc mocha",
    "build": "rollup src/Task.js --file bundle.js --sourcemap --format umd --name cadept",
    "docs": "npm run docs:gen && npm run docs:publish",
    "docs:gen": "npm run docs:clean && jsdoc -c jsdoc.json && cd docs/cadept && ln -sfn $npm_package_version latest",
    "docs:clean": "rm -rf docs/cadept/$npm_package_version",
    "docs:publish": "git subtree push --prefix docs/cadept origin gh-pages"
  },
  "_setup": {
    "deps": {
      "nyc": {
        "version": "15.1.0"
      },
      "npm-watch": {
        "version": "0.6.0"
      },
      "codecov": {
        "version": "3.7.2"
      },
      "rollup": {
        "version": "2.23.0"
      },
      "mocha": {
        "version": "8.1.1"
      }
    }
  },
  "watch": {
    "test": {
      "patterns": [
        "test.js",
        "bundle.js"
      ],
      "inherit": true
    },
    "build": {
      "patterns": "src",
      "inherit": true
    },
    "docs:gen": {
      "patterns": [
        "src",
        "README.md",
        "jsdoc.json"
      ],
      "inherit": true
    }
  },
  "keywords": [
    "async",
    "dependency",
    "task",
    "callable"
  ],
  "author": "amekusa (https://amekusa.com)",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "conso1e": "^1.2.2",
    "flex-params": "^3.0.1",
    "generic-exceptions": "^2.0.1"
  },
  "devDependencies": {
    "docolatte": "^2.9.0"
  }
}
